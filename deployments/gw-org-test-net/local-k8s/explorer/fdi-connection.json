{
  "name": "fdi_openplatform-all",
  "version": "1.0.0",
  "client": {
    "tlsEnable": true,
    "adminUser": "rca-etradeconnect-admin",
    "adminPassword": "rca-etradeconnect-adminPW",
    "enableAuthentication": false,
    "organization": "EtcMSP",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "channels": {
    "loanapp": {
      "orderers": [
        "orderer0-hktfp"
      ],
      "peers": {
        "peer0-etradeconnect": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer1-etradeconnect": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer0-pbctfp": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        },
        "peer1-pbctfp": {
          "endorsingPeer": true,
          "chaincodeQuery": true,
          "ledgerQuery": true,
          "eventSource": true
        }
      },
      "chaincodes": [
        "eventstore:1.0"
      ]
    }
  },
  "organizations": {
    "EtcMSP": {
      "mspid": "EtcMSP",
      "fullpath": false,
      "peers": [
        "peer0-etradeconnect",
        "peer1-etradeconnect"
      ],
      "certificateAuthorities": [
        "rca-etradeconnect"
      ],
      "adminPrivateKey": {
        "path": "/var/artifacts/crypto-config/EtcMSP/admin/msp/keystore/key.pem"
      },
      "signedCert": {
        "path": "/var/artifacts/crypto-config/EtcMSP/admin/msp/signcerts/cert.pem"
      }
    },
    "PbctfpMSP": {
      "mspid": "PbctfpMSP",
      "fullpath": false,
      "peers": [
        "peer0-pbctfp",
        "peer1-pbctfp"
      ],
      "certificateAuthorities": [
        "rca-pbctfp"
