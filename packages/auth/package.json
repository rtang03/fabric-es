{
  "name": "@fabric-es/auth",
  "version": "0.6.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "BSD-3-Clause-Clear",
  "description": "revamped auth server",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/fabric-es/fabric-es.git"
  },
  "keywords": [
    "hyperledger"
  ],
  "author": "Ross Tang",
  "homepage": "https://github.com/fabric-es/fabric-es",
  "bugs": {
    "url": "https://github.com/fabric-es/fabric-es/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist && rimraf build",
    "tsc:prod": "yarn clean && tsc -p ./tsconfig.prod.json",
    "test": "jest --detectOpenHandles unit.test",
    "pack:lib": "yarn build:cleanup && concurrently \"npm:cp:*\"",
    "build:cleanup": "rimraf ./build && mkdir build",
    "cp:package.json": "cp ./package.json ./build/",
    "cp:env": "cp ./.env.prod ./build/.env",
    "cp:dist": "cp -R ./dist ./build/",
    "cp:processes": "cp ./processes.yaml ./build/"
  },
  "private": true,
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.1.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "http-status": "^1.4.1",
    "ioredis": "^4.16.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "oauth2orize": "^1.11.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^8.0.2",
    "pgtools": "^0.3.0",
    "stoppable": "^1.1.0",
    "typeorm": "^0.2.20",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/ioredis": "^4.14.9",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.138",
    "@types/morgan": "^1.9.0",
    "@types/node": "^10.17.18",
    "@types/oauth2orize": "^1.8.7",
    "@types/passport": "^1.0.3",
    "@types/passport-http": "^0.3.8",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/passport-local": "^1.0.33",
    "@types/passport-oauth2-client-password": "^0.1.2",
    "@types/pg": "^7.14.3",
    "@types/supertest": "^2.0.8",
    "concurrently": "^5.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2"
  },
  "resolutions": {
    "tslib": "1.11.2"
  }
}
