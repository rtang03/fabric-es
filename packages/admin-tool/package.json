{
  "name": "@espresso/admin-tool",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean && tsc",
    "test:install-instantiate-eventstore": "npm run prepare-asset && jest eventstore-instantiate.test",
    "test:install-upgrade-eventstore": "npm run prepare-asset && jest eventstore-upgrade.test",
    "test:install-instantiate-privatedata": "npm run prepare-asset && jest privatedata-instantiate.test",
    "test:install-upgrade-privatedata": "npm run prepare-asset && jest privatedata-upgrade.test",
    "test:ca": "rimraf wallet && jest ca.unit.test",
    "test:ca-identity": "jest ca.identity.unit.test",
    "prepare-asset": "rimraf assets && mkdir -p ./assets/client-certs/hfc-kvs/org1",
    "clean": "rimraf dist"
  },
  "private": true,
  "engineStrict": true,
  "dependencies": {
    "dotenv": "^8.1.0",
    "fabric-client": "^1.4.4",
    "fabric-contract-api": "^1.4.2",
    "fabric-network": "^1.4.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "prettyjson": "^1.2.1",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^8.10.54",
    "@types/winston": "^2.4.4",
    "child-process-promise": "^2.2.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.0-beta"
  }
}
