{
  "name": "@espresso/caliper",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean && tsc",
    "clean": "rimraf dist",
    "enrollAdmin": "rimraf assets/wallet/admin.Org1 && ./node_modules/.bin/ts-node src/enrollAdmin.ts",
    "bind": "npx caliper bind --caliper-bind-sut fabric --caliper-bind-sdk 1.4.4",
    "test": "npm run build && npx caliper benchmark run --caliper-benchconfig benchmarks/api/base.yaml --caliper-networkconfig caliper.yaml --caliper-workspace . --caliper-flow-only-test --caliper-fabric-usegateway --caliper-fabric-discovery "
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "engineStrict": true,
  "dependencies": {
    "@espresso/admin-tool": "1.0.0",
    "@espresso/fabric-cqrs": "1.0.0",
    "@hyperledger/caliper-cli": "^0.2.0",
    "dotenv": "^8.1.0",
    "fabric-ca-client": "^1.4.4",
    "fabric-client": "^1.4.4",
    "fabric-network": "^1.4.4",
    "fabric-protos": "^2.0.0-snapshot.1",
    "grpc": "^1.14.2",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "prettyjson": "^1.2.1"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.12.3",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.7.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  }
}
