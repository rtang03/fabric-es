{
  "name": "@fabric-es/proxy",
  "version": "0.6.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "BSD-3-Clause-Clear",
  "description": "reverse proxy",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/fabric-es/fabric-es.git"
  },
  "keywords": [
    "hyperledger"
  ],
  "author": "Ross Tang",
  "homepage": "https://github.com/fabric-es/fabric-es",
  "bugs": {
    "url": "https://github.com/fabric-es/fabric-es/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "start": "node ./dist/index.js",
    "tsc:prod": "yarn clean && tsc",
    "clean": "rimraf ./dist && rimraf ./build",
    "dev": "ts-node src/index.ts",
    "build:cleanup": "rimraf ./build && mkdir build",
    "pack:lib": "yarn build:cleanup && concurrently \"npm:cp:*\"",
    "cp:package.json": "cp ./package.json ./build/",
    "cp:env": "cp ./.env.prod ./build/.env",
    "cp:dist": "cp -R ./dist ./build/"
  },
  "private": true,
  "dependencies": {
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^1.0.3",
    "morgan": "^1.10.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/node": "^10.17.18",
    "concurrently": "^5.0.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2"
  }
}
