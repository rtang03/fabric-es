{
  "name": "@espresso/gw-org1",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean && tsc",
    "clean": "rimraf dist",
    "service-docu": "node ./dist/service-doc.js",
    "service-loan": "node ./dist/service-loan.js",
    "service-user": "node ./dist/service-user.js",
    "service-priv": "node ./dist/service-private.js",
    "service-remote": "node ./dist/service-remote-data.js",
    "service-admin": "node ./dist/service-admin.js",
    "start-services": "yarn build && concurrently \"npm:service-*\"",
    "server-gw": "node ./dist/start-gateway.js",
    "server-auth": "node ./dist/start-auth-server",
    "start-server": "yarn build && concurrently \"npm:server-*\"",
    "enrollAdmin": "rimraf assets/wallet/admin-org1.example.com && node ./dist/enrollAdmin.js",
    "enrollCaAdmin": "rimraf assets/wallet/rca-org1-admin && node ./dist/enrollCaAdmin.js",
    "gen-resolvers-type": "graphql-codegen --config codegen.yml"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "engineStrict": true,
  "dependencies": {
    "@apollo/federation": "^0.10.2",
    "@apollo/gateway": "^0.10.8",
    "@espresso/fabric-cqrs": "1.0.0",
    "@espresso/gw-node": "1.0.0",
    "@espresso/model-common": "1.0.0",
    "@espresso/model-loan": "1.0.0",
    "@espresso/operator": "1.0.0",
    "apollo-server": "^2.9.7",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fabric-network": "^1.4.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "prettyjson": "^1.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/bytebuffer": "^5.0.40",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^8.10.54",
    "@types/node-fetch": "^2.5.4",
    "apollo-server-testing": "^2.9.7",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  }
}
