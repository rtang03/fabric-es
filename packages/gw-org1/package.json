{
  "name": "@fabric-es/gw-org1",
  "version": "0.6.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "BSD-3-Clause-Clear",
  "author": "Paul Lai",
  "description": "gw-org1",
  "scripts": {
    "build": "yarn clean && tsc",
    "tsc:prod": "yarn clean && tsc -p ./tsconfig.prod.json",
    "pack:gw-dev-net": "yarn build:cleanup && concurrently \"npm:cp:*\"",
    "build:cleanup": "rimraf ../../.build/packages/gw-org1 && mkdir -p ../../.build/packages/gw-org1/connection",
    "cp:main": "cp ./package.json ./processes.yaml ../../.build/packages/gw-org1/",
    "cp:dist": "cp -R ./dist ../../.build/packages/gw-org1/",
    "cp:env": "cp ../../deployments/dev-net/build.gw-org1/.env.prod ../../.build/packages/gw-org1/.env",
    "cp:conn": "cp ../../deployments/dev-net/build.gw-org1/connection-org1.docker.yaml ../../.build/packages/gw-org1/connection",
    "cp:pm2": "cp ../../entrypoint.sh ../../.build/",
    "clean": "rimraf dist",
    "service-docu": "node ./dist/service-doc.js",
    "service-loan": "node ./dist/service-loan.js",
    "service-user": "node ./dist/service-user.js",
    "service-ctnt": "node ./dist/service-prv-ctnt.js",
    "service-rdtls": "node ./dist/service-rmt-dtls.js",
    "service-admin": "node ./dist/service-admin.js",
    "start-services": "yarn build && concurrently \"npm:service-*\"",
    "server-gw": "node ./dist/start-gateway.js",
    "start-server": "concurrently \"npm:server-*\"",
    "enrollAdmin": "node ./dist/enrollAdmin.js",
    "enrollCaAdmin": "node ./dist/enrollCaAdmin.js",
    "gen-resolvers-type": "graphql-codegen --config codegen.yml",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}'",
    "test:intg": "yarn build && ./node_modules/.bin/jest intg.test",
    "app": "node ./dist/app.js"
  },
  "private": true,
  "dependencies": {
    "@apollo/federation": "^0.10.2",
    "@apollo/gateway": "^0.10.8",
    "@fabric-es/fabric-cqrs": "^0.6.0",
    "@fabric-es/gateway-lib": "^0.6.0",
    "@fabric-es/model-common": "^0.6.0",
    "@fabric-es/model-loan": "^0.6.0",
    "@fabric-es/operator": "^0.6.0",
    "apollo-server": "^2.9.7",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fabric-network": "2.0.0-snapshot.348",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "stoppable": "^1.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/bytebuffer": "^5.0.40",
    "@types/concurrently": "^4.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^10.17.18",
    "@types/node-fetch": "^2.5.4",
    "@types/stoppable": "^1.1.0",
    "apollo-server-testing": "^2.9.7",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2"
  }
}
