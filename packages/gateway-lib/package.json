{
  "name": "@fabric-es/gateway-lib",
  "version": "0.5.15",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "Gateway library",
  "repository": {
    "type": "git",
    "url": "git://github.com/fabric-es/fabric-es.git"
  },
  "keywords": [
    "hyperledger"
  ],
  "author": "Ross Tang",
  "license": "BSD-3-Clause-Clear",
  "homepage": "https://github.com/fabric-es/fabric-es",
  "bugs": {
    "url": "https://github.com/fabric-es/fabric-es/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "tsc:prod": "yarn clean && tsc -p ./tsconfig.prod.json",
    "pack:lib": "yarn build:cleanup && concurrently \"npm:cp:*\"",
    "build:cleanup": "rimraf ../../.build/packages/gateway-lib && mkdir -p ../../.build/packages/gateway-lib",
    "cp:package.json": "cp ./package.json ../../.build/packages/gateway-lib/",
    "cp:dist": "cp -R ./dist ../../.build/packages/gateway-lib/",
    "clean": "rimraf dist",
    "typedoc": "typedoc --excludeNotExported --theme markdown --exclude \"**/*+(.test|constants).ts\" --out docs src/index.ts",
    "generate-types-for-admin": "graphql-codegen --config src/generated/codegen.yml",
    "enrollAdmin": "node ./dist/enrollAdmin.js",
    "enrollCaAdmin": "node ./dist/enrollCaAdmin.js",
    "start-admin-service": "npm run build && node ./dist/admin/__tests__/__utils__/startServiceForManualTest.js",
    "unit-test": "./unit-test.sh",
    "test:admin-service": "jest --forceExit --detectOpenHandles admin.unit.test",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}'",
    "prepublish": "yarn typedoc",
    "publish:gh:fabric-es": "npm publish"
  },
  "dependencies": {
    "@apollo/federation": "^0.10.2",
    "@apollo/gateway": "^0.10.8",
    "@fabric-es/authentication": "^0.5.15",
    "@fabric-es/fabric-cqrs": "^0.5.15",
    "@fabric-es/operator": "^0.5.15",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.7",
    "arraybuffer-to-string": "^1.0.2",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fabric-network": "2.0.0-snapshot.348",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "prettyjson": "^1.2.1",
    "rimraf": "^3.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.0",
    "@graphql-codegen/introspection": "^1.9.0",
    "@graphql-codegen/typescript": "^1.9.0",
    "@graphql-codegen/typescript-document-nodes": "^1.9.1",
    "@graphql-codegen/typescript-operations": "^1.9.1",
    "@graphql-codegen/typescript-react-apollo": "^1.9.1",
    "@graphql-codegen/typescript-resolvers": "^1.9.0",
    "@graphql-codegen/typescript-type-graphql": "^1.9.1",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/bytebuffer": "^5.0.40",
    "@types/concurrently": "^4.1.0",
    "@types/cookie": "^0.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.138",
    "@types/morgan": "^1.7.37",
    "@types/node": "^8.10.54",
    "@types/node-fetch": "^2.5.4",
    "@types/rimraf": "^2.0.3",
    "apollo-server-testing": "^2.9.7",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.6",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.7.2"
  }
}
