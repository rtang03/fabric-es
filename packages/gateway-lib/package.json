{
  "name": "@fabric-es/gateway-lib",
  "version": "0.6.2",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "Gateway library",
  "repository": {
    "type": "git",
    "url": "git://github.com/fabric-es/fabric-es.git"
  },
  "keywords": [
    "hyperledger"
  ],
  "author": "Ross Tang",
  "license": "BSD-3-Clause-Clear",
  "homepage": "https://github.com/fabric-es/fabric-es",
  "bugs": {
    "url": "https://github.com/fabric-es/fabric-es/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "tsc:prod": "yarn clean && tsc -p ./tsconfig.prod.json",
    "clean": "rimraf dist",
    "typedoc": "typedoc --excludeNotExported --theme markdown --exclude \"**/*+(.test|constants).ts\" --out docs src/index.ts",
    "generate-types-for-admin": "(broke) graphql-codegen --config src/generated/codegen.yml",
    "enrollAdmin": "node ./dist/enrollAdmin.js",
    "enrollCaAdmin": "node ./dist/enrollCaAdmin.js",
    "unit-test:fab": "yarn unit-test-counter && yarn unit-test-qh",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}'",
    "prepublish": "yarn typedoc && yarn tsc:prod",
    "publish:gh:fabric-es": "npm publish",
    "dev:query-handler": "yarn build && node ./dist/query-handler/app.js",
    "unit-test-qh": "jest --forceExit --detectOpenHandles queryHandler.unit-test",
    "unit-test-counter": "jest --forceExit --detectOpenHandles counter.unit-test"
  },
  "dependencies": {
    "@apollo/federation": "^0.16.0",
    "@apollo/gateway": "^0.16.0",
    "@fabric-es/fabric-cqrs": "^0.6.2",
    "@fabric-es/operator": "^0.6.2",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.13.1",
    "arraybuffer-to-string": "^1.0.2",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fabric-network": "2.1.0",
    "graphql": "^15.0.0",
    "graphql-redis-subscriptions": "^2.2.1",
    "graphql-subscriptions": "^1.1.0",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^5.0.0",
    "grpc": "^1.24.2",
    "http-status": "^1.4.1",
    "ioredis": "^4.17.3",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "rimraf": "^3.0.0",
    "subscriptions-transport-ws": "^0.9.16",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/bytebuffer": "^5.0.41",
    "@types/cookie": "^0.4.0",
    "@types/express": "^4.17.6",
    "@types/ioredis": "^4.16.4",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.152",
    "@types/morgan": "^1.7.37",
    "@types/node": "^10.17.18",
    "@types/node-fetch": "^2.5.7",
    "@types/rimraf": "^3.0.0",
    "apollo-server-testing": "^2.13.1",
    "jest": "^24.9.0",
    "prettier": "^2.0.5",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.17.7",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.8.3"
  }
}
