{
  "name": "@fabric-es/operator",
  "version": "0.6.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "Network operator",
  "repository": {
    "type": "git",
    "url": "git://github.com/fabric-es/fabric-es.git"
  },
  "keywords": [
    "hyperledger"
  ],
  "author": "Ross Tang",
  "license": "BSD-3-Clause-Clear",
  "homepage": "https://github.com/fabric-es/fabric-es",
  "bugs": {
    "url": "https://github.com/fabric-es/fabric-es/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "tsc:prod": "yarn clean && tsc -p ./tsconfig.prod.json",
    "build-chaincode": "cd ../chaincode && yarn build",
    "prepare-asset": "rimraf assets && mkdir -p ./assets/client-certs/hfc-kvs/org1",
    "typedoc": "typedoc --excludeNotExported --theme markdown --exclude \"**/*+(.test|constants).ts\" --out docs src/index.ts",
    "unit-test:fab": "jest unit.test",
    "clean": "rimraf dist",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}' --fix",
    "prepublish": "yarn typedoc && yarn tsc:prod",
    "publish:gh:fabric-es": "npm publish"
  },
  "dependencies": {
    "dotenv": "^8.1.0",
    "fabric-ca-client": "2.0.0-snapshot.391",
    "fabric-client": "2.0.0-snapshot.326",
    "fabric-contract-api": "^2.0.0",
    "fabric-network": "2.0.0-snapshot.348",
    "fs-extra": "^8.1.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "rimraf": "^3.0.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/concurrently": "^4.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^10.17.18",
    "concurrently": "^5.0.0",
    "execa": "3.4.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typedoc": "^0.15.6",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.7.2"
  }
}
