{
  "name": "@espresso/gw-node",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean && tsc",
    "clean": "rimraf dist",
    "test:unit": "jest unit.test",
    "test:integration": "jest integration.test",
    "service-docu": "node ./dist/service-doc.js",
    "service-loan": "node ./dist/service-loan.js",
    "service-user": "node ./dist/service-user.js",
    "service-priv": "node ./dist/service-private.js",
    "start-services": "yarn build && concurrently \"npm:service-*\"",
    "start-gateway": "yarn build && node ./dist/start-gateway.js",
    "enrollAdmin": "rimraf assets/_wallet/admin && ./node_modules/.bin/ts-node src/enrollAdmin.ts",
    "svs-admin": "nodemon --exec ts-node ./src/service-admin.ts",
    "svs-remote-data": "nodemon --exec ts-node ./src/service-remote-data.ts",
    "start-admin-gateway": "nodemon --exec ts-node src/admin-only-gateway.ts",
    "generate-types-for-admin": "graphql-codegen --config src/generated/codegen.yml",
    "enrollCaAdmin": "rimraf assets/_wallet/ca_admin && ./node_modules/.bin/ts-node src/admin/enrollCaAdmin.ts",
    "test-service-admin:integration": "jest basic-int.test",
    "test-service-admin:auth:integration": "jest --forceExit --detectOpenHandles auth-int.test"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "engineStrict": true,
  "dependencies": {
    "@apollo/federation": "^0.10.2",
    "@apollo/gateway": "^0.10.8",
    "@espresso/admin-tool": "1.0.0",
    "@espresso/authentication": "1.0.0",
    "@espresso/fabric-cqrs": "1.0.0",
    "@espresso/model-common": "1.0.0",
    "@espresso/model-loan": "1.0.0",
    "@espresso/model-loan-private": "1.0.0",
    "apollo-link-http": "^1.5.16",
    "apollo-server": "^2.9.7",
    "arraybuffer-to-string": "^1.0.2",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cookie": "^0.4.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "fabric-network": "^1.4.4",
    "graphql": "^14.5.8",
    "graphql-resolvers": "^0.3.3",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.6.0",
    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "prettyjson": "^1.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/bytebuffer": "^5.0.40",
    "@types/cookie": "^0.3.3",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/lodash": "^4.14.138",
    "@types/morgan": "^1.7.37",
    "@types/node": "^8.10.54",
    "@types/node-fetch": "^2.5.4",
    "apollo-server-testing": "^2.9.7",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.7.2"
  }
}
