{
  "name": "@espresso/fabric-cqrs",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "yarn run clean && tsc",
    "build:prod": "yarn run clean && tsc -p ./tsconfig.prod.json",
    "package": "yarn run build:cleanup && yarn run cp:package.json && yarn run cp:dist",
    "build:cleanup": "rimraf ../../.build/packages/fabric-cqrs && mkdir -p ../../.build/packages/fabric-cqrs",
    "cp:package.json": "cp ./package.json ../../.build/packages/fabric-cqrs/",
    "cp:dist": "cp -R ./dist ../../.build/packages/fabric-cqrs/",
    "clean": "rimraf dist",
    "test:account": "jest account.unit.test",
    "test:service.integration": "jest service.integration.test",
    "test:service.multiusers": "rimraf assets/walletOrg1 && rimraf assets/walletOrg2 && jest service.multiusers.test",
    "test:service.privatedata.integration": "jest service.privatedata.integration.test",
    "test:service.ngac.crud.integration": "(broke) jest service.ngac.crud.integration.test",
    "test:service.ngac.pe.int.1u": "(broke) jest one-user.service.ngac.pe.int.test",
    "test:store.integration": "jest command.integration.test",
    "test:store.privatedata.integration": "jest command.privatedata.integration.test",
    "test:peer.integration": "jest peer.integration.test",
    "test:peer.privatedata.integration": "jest peer.privatedata.integration.test",
    "test:peer.ngac.integration": "(not implemented yet) jest peer.ngac.integration.test",
    "test:all-unit-test": "jest unit.test",
    "enrollAdmin": "ts-node src/account/enrollAdmin.ts",
    "enrollCaAdmin": "ts-node src/account/enrollCaAdmin.ts",
    "enrollAdminOrg2": "(broke) ts-node src/ngac_test/enrollAdmin.ts",
    "typedoc": "typedoc --excludeNotExported --theme markdown --exclude \"**/*+(.test|constants).ts\" --out docs src/index.ts"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "engineStrict": true,
  "dependencies": {
    "@espresso/operator": "1.0.0",
    "dotenv": "^8.1.0",
    "fabric-client": "^1.4.4",
    "fabric-contract-api": "^1.4.2",
    "fabric-network": "^1.4.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "prettyjson": "^1.2.1",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "rxjs": "^6.5.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.40",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/lodash": "^4.14.138",
    "@types/node": "^8.10.54",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typedoc": "^0.15.7",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.7.2"
  }
}
