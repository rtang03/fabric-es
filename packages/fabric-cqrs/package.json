{
  "name": "@fabric-es/fabric-cqrs",
  "version": "0.6.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "BSD-3-Clause-Clear",
  "description": "Hyperledger Fabric middleware for cqrs pattern",
  "repository": {
    "type": "git",
    "url": "git://github.com/fabric-es/fabric-es.git"
  },
  "keywords": [
    "hyperledger"
  ],
  "author": "Ross Tang",
  "homepage": "https://github.com/fabric-es/fabric-es",
  "bugs": {
    "url": "https://github.com/fabric-s/fabric-es/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "yarn clean && tsc",
    "tsc:prod": "yarn clean && tsc -p ./tsconfig.prod.json",
    "clean": "rimraf dist",
    "test:service.integration": "jest service.integration.test",
    "test:service.multiusers": "rimraf assets/walletOrg1 && rimraf assets/walletOrg2 && jest service.multiusers.test",
    "test:service.privatedata.integration": "jest service.privatedata.integration.test",
    "test:store.integration": "jest command.integration.test",
    "test:store.privatedata.integration": "jest command.privatedata.integration.test",
    "test:store.unit": "jest store.unit.test",
    "test:peer.integration": "jest peer.integration.test",
    "test:peer.privatedata.integration": "jest peer.privatedata.integration.test",
    "unit-test:fab-old": "sudo yarn enrollAdmin && sudo yarn enrollCaAdmin && jest unit.test && jest integration.test && yarn test:service.multiusers",
    "unit-test:fab": "jest unit.test && jest integration.test && yarn test:service.multiusers",
    "enrollAdmin": "node ./dist/account/enrollAdmin.js",
    "enrollCaAdmin": "node ./dist/account/enrollCaAdmin.js",
    "typedoc": "typedoc --excludeNotExported --theme markdown --exclude \"**/*+(.test|constants).ts\" --out docs src/index.ts",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}'",
    "prepublish": "yarn typedoc && yarn tsc:prod",
    "publish:gh:fabric-es": "npm publish"
  },
  "dependencies": {
    "@fabric-es/operator": "^0.6.1",
    "dotenv": "^8.1.0",
    "fabric-client": "2.0.0-snapshot.326",
    "fabric-contract-api": "^2.1.2",
    "fabric-network": "2.0.0-snapshot.348",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "redux": "^4.0.4",
    "redux-observable": "^1.2.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bytebuffer": "^5.0.41",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.4",
    "@types/lodash": "^4.14.152",
    "@types/node": "^10.17.18",
    "jest": "^24.9.0",
    "prettier": "^2.0.5",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.17.7",
    "typedoc-plugin-markdown": "^2.2.16",
    "typescript": "^3.8.3"
  }
}
