{
  "name": "@fabric-es/gw-org3",
  "version": "0.5.14",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "BSD-3-Clause-Clear",
  "author": "Paul Lai",
  "description": "gw-org3",
  "scripts": {
    "build": "yarn run clean && tsc",
    "tsc:prod": "yarn run clean && tsc -p ./tsconfig.prod.json",
    "pack:gw-dev-net": "yarn build:cleanup && concurrently \"npm:cp:*\"",
    "build:cleanup": "rimraf ../../.build/packages/gw-org3 && mkdir -p ../../.build/packages/gw-org3",
    "cp:package.json": "cp ./package.json ../../.build/packages/gw-org3/",
    "cp:dist": "cp -R ./dist ../../.build/packages/gw-org3/",
    "cp:env": "cp ../../_archive/gw-dev-net/build.gw-org3/.env.prod ../../.build/packages/gw-org3/.env",
    "cp:conn": "cp ../../_archive/gw-dev-net/build.gw-org3/connection-org3.prod.yaml ../../.build/packages/gw-org3/connection.yaml",
    "cp:pm2": "cp ./processes.yaml ../../.build/packages/gw-org3/",
    "clean": "rimraf dist",
    "test:unit": "yarn build && ./node_modules/.bin/jest unit.test",
    "test:multi": "yarn build && ./node_modules/.bin/jest multi.test",
    "service-docu": "node ./dist/service-doc.js",
    "service-loan": "node ./dist/service-loan.js",
    "service-user": "node ./dist/service-user.js",
    "service-ctnt": "node ./dist/service-prv-ctnt.js",
    "service-dtls": "node ./dist/service-prv-dtls.js",
    "service-admin": "node ./dist/service-admin.js",
    "start-services": "yarn build && concurrently \"npm:service-*\"",
    "server-gw": "node ./dist/start-gateway.js",
    "start-server": "yarn build && concurrently \"npm:server-*\"",
    "enrollAdmin": "rimraf assets/wallet/admin-org3.example.com && node ./dist/enrollAdmin.js",
    "enrollCaAdmin": "rimraf assets/wallet/rca-org3-admin && node ./dist/enrollCaAdmin.js",
    "gen-resolvers-type": "graphql-codegen --config codegen.yml",
    "pm2": "yarn pm2:enroll &&  yarn pm2:enrollCa && yarn pm2:services && yarn pm2:gateway",
    "pm2:enroll": "node ./dist/enrollAdmin.js && sleep 1",
    "pm2:enrollCa": "node ./dist/enrollCaAdmin.js && sleep 1",
    "pm2:services": "pm2 start processes.yaml --only \"admin,doc,loan,prv-ctnt,prv-dtls,user\"",
    "pm2:gateway": "pm2-runtime --delay 3 --only \"gw\" processes.yaml",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}'",
    "unit-test": "jest unit.test"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "private": true,
  "engineStrict": true,
  "dependencies": {
    "@apollo/federation": "^0.10.2",
    "@apollo/gateway": "^0.10.8",
    "@fabric-es/fabric-cqrs": "^0.5.14",
    "@fabric-es/gateway-lib": "^0.5.14",
    "@fabric-es/model-common": "^0.5.14",
    "@fabric-es/model-loan": "^0.5.14",
    "@fabric-es/operator": "^0.5.14",
    "apollo-server": "^2.9.7",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "fabric-network": "^1.4.4",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.6",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "prettyjson": "^1.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.1",
    "@types/bytebuffer": "^5.0.40",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^8.10.54",
    "@types/node-fetch": "^2.5.4",
    "apollo-server-testing": "^2.9.7",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2"
  }
}
