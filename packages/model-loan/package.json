{
  "name": "@fabric-es/model-loan",
  "version": "0.6.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "BSD-3-Clause-Clear",
  "author": "Paul Lai",
  "description": "Model - loan",
  "scripts": {
    "build": "yarn clean && tsc",
    "tsc:prod": "yarn clean && tsc -p ./tsconfig.prod.json",
    "pack:gw-dev-net": "yarn build:cleanup && concurrently \"npm:cp:*\"",
    "build:cleanup": "rimraf ../../.build/packages/model-loan && mkdir -p ../../.build/packages/model-loan",
    "cp:package.json": "cp ./package.json ../../.build/packages/model-loan/",
    "cp:dist": "cp -R ./dist ../../.build/packages/model-loan/",
    "test": "yarn build && ./node_modules/.bin/jest unit.test",
    "clean": "rimraf dist",
    "gen-resolvers-type": "graphql-codegen --config codegen.yml",
    "lint": "eslint --max-warnings 10 'src/**/*.{js,ts}'",
    "unit-test:fab": "jest unit.test"
  },
  "private": true,
  "dependencies": {
    "@fabric-es/fabric-cqrs": "^0.6.0",
    "@fabric-es/gateway-lib": "^0.6.0",
    "@fabric-es/model-common": "^0.6.0",
    "apollo-server-errors": "^2.3.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.9.0",
    "@graphql-codegen/introspection": "^1.9.0",
    "@graphql-codegen/typescript": "^1.9.0",
    "@graphql-codegen/typescript-document-nodes": "^1.9.1",
    "@graphql-codegen/typescript-operations": "^1.9.1",
    "@graphql-codegen/typescript-react-apollo": "^1.9.1",
    "@graphql-codegen/typescript-resolvers": "^1.9.0",
    "@graphql-codegen/typescript-type-graphql": "^1.9.1",
    "@types/concurrently": "^4.1.0",
    "@types/jest": "^24.0.18",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.7.2"
  }
}
