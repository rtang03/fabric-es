    "orgs": {
        "tls": {
            "domain": "hktfp.com",
            "ca": {
                "host": "tls-ca",
                "port": "6052",
                "admin": "tls-ca-admin",
                "adminpw": "tls-ca-adminPW"
            }
        },
        "orderer": {
            "name": "hktfp",
            "id": "HktfpMSP",
            "domain": "hktfp.com",
            "orderers": {
                "type": "etcdraft",
                "use-kafka": false,
                "use-raft": true,
                "batch-timeout": "2s",
                "max-message-count": "500",
                "absolute-max-bytes": "99 MB",
                "preferred-max-bytes": "2 MB",
                "max-channels": "10",
                "genesis-profile": "SampleInsecureKafka",
                "raft-tick-interval": "500ms",
                "raft-election-tick": "10",
                "raft-heartbeat-tick": "1",
                "raft-max-inflight-blocks": "5",
                "raft-snapshot-interval-size": "20 MB",
                "endpoints": [
                    {
                        "-": "orderer0",
                        "ext-port": "7050"
                    },
                    {
                        "-": "orderer1",
                        "ext-port": "7150"
                    },
                    {
                        "-": "orderer2",
                        "ext-port": "7250"
                    },
                    {
                        "-": "orderer3",
                        "ext-port": "7350"
                    },
                    {
                        "-": "orderer4",
                        "ext-port": "7450",
                        "islast": true
                    }
                ]
            },
            "kafkas": {
                "retry-shortinterval": "1s",
                "retry-shorttotal": "30s",
                "endpoints": [
                ],
                "ports": [
                    "9092",
                    "9093"
                ]
            },
            "zookeepers": {
                "endpoints": [
                ],
                "ports": [
                    "2181",
                    "2888",
                    "3888"
                ]
            },
            "ca": {
                "host": "rca",
                "port": "6053",
                "admin": "rca-hktfp-admin",
                "adminpw": "rca-hktfp-adminPW"
            }
        },
