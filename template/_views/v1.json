{
    "network": {
        "name": "fabric",
        "host-delimiter": "-",
        "compose-project-name": "openplatform",
        "consortium": "SampleConsortium",
        "orderer-channel": "ordererchannel",
        "peer-channels": [
            "mychannel"
        ],
        "nodes": [
        ]
    },

    "artifacts": {
        "local": "../artifacts",
        "docker": "/var/artifacts",
        "crypto": "crypto-config",
        "production": "production",
        "orderer-home": "orderer",
        "peers-working-dir": "/opt/gopath/src/github.com/hyperledger/fabric"
    },

    "orgs": {
        "tls": {
            "domain": "tls",
            "ca": {
                "host": "ca",
                "port": "6052",
                "admin": "tls-ca-admin",
                "adminpw": "tls-ca-adminPW",
                "admin-cert-path": "ca-tls/admin",
                "server-home": "ca-tls/server"
            }
        },
        "orderer": {
            "name": "org0",
            "id": "org0MSP",
            "domain": "org0",
            "msp-path": "org0/msp",
            "admin-cert-path": "org0/admin",
            "ca-server-cert-path": "org0/ca/crypto",
            "orderers": {
                "type": "etcdraft",
                "use-kafka": false,
                "use-raft": true,
                "batch-timeout": "2s",
                "max-message-count": "500",
                "absolute-max-bytes": "99 MB",
                "preferred-max-bytes": "2 MB",
                "max-channels": "10",
                "genesis-profile": "SampleInsecureKafka",
                "raft-tick-interval": "500ms",
                "raft-election-tick": "10",
                "raft-heartbeat-tick": "1",
                "raft-max-inflight-blocks": "5",
                "raft-snapshot-interval-size": "20 MB",
                "endpoints": [
                    {
                        "-": "orderer1",
                        "ext-port": "7050",
                        "cert-path": "org0/orderer1"
                    },
                    {
                        "-": "orderer2",
                        "ext-port": "7150",
                        "cert-path": "org0/orderer2"
                    },
                    {
                        "-": "orderer3",
                        "ext-port": "7250",
                        "cert-path": "org0/orderer3"
                    },
                    {
                        "-": "orderer4",
                        "ext-port": "7350",
                        "cert-path": "org0/orderer4"
                    },
                    {
                        "-": "orderer5",
                        "ext-port": "7450",
                        "cert-path": "org0/orderer5",
                        "islast": true
                    }
                ]
            },
            "kafkas": {
                "retry-shortinterval": "1s",
                "retry-shorttotal": "30s",
                "endpoints": [
                ],
                "ports": [
                    "9092",
                    "9093"
                ]
            },
            "zookeepers": {
                "endpoints": [
                ],
                "ports": [
                    "2181",
                    "2888",
                    "3888"
                ]
            },
            "ca": {
                "host": "rca",
                "port": "6053",
                "admin": "rca-org0-admin",
                "adminpw": "rca-org0-adminPW",
                "admin-cert-path": "org0/ca/admin",
                "server-home": "org0/ca/server"
            }
        },
        "peer": [
            {
                "name": "org1",
                "script-naming": "org1",
                "id": "org1MSP",
                "domain": "org1",
                "msp-path": "org1/msp",
                "admin-cert-path": "org1/admin",
                "ca-server-cert-path": "org1/ca/crypto",
                "peers": {
                    "anchor": "peer1",
                    "anchor-port": "7051",
                    "endpoints": [
                        {
                            "-": "peer1",
                            "port": "7051",
                            "chaincode-port": "7052",
                            "eventhub-port": "7053",
                            "cert-path": "org1/peer1"
                        },
                        {
                            "-": "peer2",
                            "port": "7151",
                            "chaincode-port": "7152",
                            "eventhub-port": "7153",
                            "cert-path": "org1/peer2",
                            "islast": true
                        }
                    ]
                },
                "ca": {
                    "host": "rca",
                    "port": "6054",
                    "admin": "rca-org1-admin",
                    "adminpw": "rca-org1-adminPW",
                    "admin-cert-path": "org1/ca/admin",
                    "server-home": "org1/ca/server"
                },
                "gw": {
                    "host": "localhost",
                    "port": "4001",
                    "auth": {
                        "host": "localhost",
                        "port": "3311",
                        "path": "/oauth/authenticate",
                        "dbserver": "postgres01",
                        "dbname": "gw-org1",
                        "dbuser": "postgres",
                        "password": "docker",
